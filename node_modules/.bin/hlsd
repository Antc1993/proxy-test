#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@warren-bank/hls-proxy/hls-proxy/bin/hlsd.js" "$@"
else 
  exec node  "$basedir/../@warren-bank/hls-proxy/hls-proxy/bin/hlsd.js" "$@"
fi
